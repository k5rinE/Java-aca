package ex;
import java.util.*;
public class ex02_switch {

	public static void main(String[] args) {   //이건안대는거임

		System.out.println("나이를 입력하세요");
		Scanner sc = new Scanner(System.in);
		
		int age = sc.nextInt();
		int fee = 2500;
		double rate = 0;
		
		switch(true) {
		case(age>=65)
			rate = 0;
		break;
		
		}
		
		
	}

}
