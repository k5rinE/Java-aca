package ex;

public class ex19 {

	public static void main(String[] args) {

		int[]answer = {1,4,4,3,1,4,4,2,1,3,2};
		int[]counter = new int[4];
		
		for(int i = 0; i<answer.length; i++) {
			counter[counter[i]]++;}
		for(int i=0; i<counter.length; i++) {
				
			System.out.println(i+ "의 개수 : " + counter[i]);
				
			}
		}
	}


